{
  "name": "codetransform",
	"displayName": "CodeTransform",
	"description": "Help to write custom transformer for block of code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:codetransform.helloWorld"
	],
